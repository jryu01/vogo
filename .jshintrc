{
  // Enforcing options
  "globals": {
      "angular": false,
      "inject": false,
      "module": false,
      "sinon": false,
      "expect": false,
      "ionic": false
  },
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "funcscope": true,
  "globalstrict": true,
  "immed": true,
  "iterator": true,
  "latedef": true,
  "maxcomplexity": 5,
  "maxdepth": 5,
  "maxparams": 5,
  "maxstatements": 25,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": true,
  "undef": true,
  "unused": true,

  // Relexing Options

  // Environments
  "browser": true,
  "mocha": true
}